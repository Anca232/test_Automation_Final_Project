<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Main Page Suite">
    <test name="Smoke Test">
        <groups>
            <run>
                <include name="Smoke"/>
            </run>
        </groups>
        <classes>
            <class name="Tests.DataTests.MainPageTest"></class>
        </classes>
    </test>
    <test name="Regression Test">
        <groups>
            <run>
                <include name="Regression"/>
            </run>
        </groups>
        <classes>
            <class name="Tests.DataTests.MyAccountTest"></class>
        </classes>
    </test>
</suite>
